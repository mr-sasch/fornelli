mixin 3panel()
	+b.panel&attributes(attributes)
		block
		// Заголовок
		.panel__title
			.panel__title-left Как
			.panel__title-center часто вы используете все конфорки вашей поверхности
			img.panel__title-right(src='../../images/7chto/chto-question-white.png')
			img.panel__title-right2(src='../../images/3panel/panel-header.png')
		// Контент
		.panel__content
			// Блок с людьми
			.panel__people#panel-people
				.panel-people__frame
					img.panel__people-img(src='../../images/3panel/panel-ellips-people.png')
					.panel__people-txt-cont
						.panel__people-txt 9 из 10-ти потребителей используют не более 3-х конфорок ежедневно
			// Блок с панелями
			.panel__panels-container
				.panel__panels
					.panel__panels-title Выберите свою панель!
					.panel__panels-1
						// Электрическую
						.panel__panels-1-col
							img.panel__panels-1-col-arrow-img.panel__panels-1-col-arrow-img-left(src='../../images/3panel/panel-arrow1.png')
							img.panel__panels-1-col-label-img(src='../../images/3panel/panel-small-round1.png')
							.panel__panels-1-col-title Электрическую
							.panel__panels-1-col-panels#panel__panels-1-col-panels
								img.panel__panels-1-col-panel-img.panel-electro-img(src='../../images/3panel/delizia_wh.png')
								img.panel__panels-1-col-panel-img.panel-electro-img.panel__panels-1-col-panel-img-2(src='../../images/3panel/delizia_off.png')
							// В нужном цвете - моб. электро
							.panel__panels-2.panel__panels-2-mob
								.panel__panels-2-title В нужном цвете
								.panel__panels-2-col
									img.panel__panels-2-col-img.panel__panels-2-col-img1.panel__panels-2-col-img(src='../../images/3panel/panel-round-color1.png')
									img.panel__panels-2-col-img.panel__panels-2-col-img2(src='../../images/3panel/panel-round-color2.png')
							// Блок с доп.опциями
							.panel__panels-1-col-dop
								.panel__panels-1-col-dop-arrow-img-cont
									img.panel__panels-1-col-dop-arrow-img(src='../../images/3panel/panel-arrow3.png')
								.panel__panels-1-col-dop-label
									img.panel__panels-1-col-dop-label-img(src='../../images/3panel/panel-small-round3.png')
									.panel__panels-1-col-dop-label-title Индукцию
								.panel__panels-1-col-dop-label
									img.panel__panels-1-col-dop-label-img(src='../../images/3panel/panel-small-round4.png')
									.panel__panels-1-col-dop-label-title HI LIGHT
						// Газовую
						.panel__panels-1-col
							img.panel__panels-1-col-arrow-img.panel__panels-1-col-arrow-img-right(src='../../images/3panel/panel-arrow2.png')
							img.panel__panels-1-col-label-img(src='../../images/3panel/panel-small-round2.png')
							.panel__panels-1-col-title Газовую
							img.panel__panels-1-col-panel-img(src='../../images/3panel/panel-panel-gaz.png')
							// В нужном цвете - моб. газ
							.panel__panels-2.panel__panels-2-mob
								.panel__panels-2-title В нужном цвете
								.panel__panels-2-col
									img.panel__panels-2-col-img.panel__panels-2-col-img3(src='../../images/3panel/panel-round-color3.png')
									img.panel__panels-2-col-img.panel__panels-2-col-img4(src='../../images/3panel/panel-round-color4.png')
									img.panel__panels-2-col-img.panel__panels-2-col-img5(src='../../images/3panel/panel-round-color5.png')
									img.panel__panels-2-col-img.panel__panels-2-col-img6(src='../../images/3panel/panel-round-color6.png')
							// Доп.блок со стрелкой
							img.panel__panels-1-col-panel-arrow-img(src='../../images/3panel/panel-arrow4.png')
					// В нужном цвете - Десктопная
					.panel__panels-2
						.panel__panels-2-title В нужном цвете
						.panel__panels-2-col#panel-electro
							img.panel__panels-2-col-img.panel__panels-2-col-img1.panel-electro(src='../../images/3panel/panel-round-color1.png')
							img.panel__panels-2-col-img.panel__panels-2-col-img2.panel-electro.z20(src='../../images/3panel/panel-round-color2.png')
						.panel__panels-2-col
							img.panel__panels-2-col-img.panel__panels-2-col-img3.panel-gas(src='../../images/3panel/panel-round-color3.png')
							img.panel__panels-2-col-img.panel__panels-2-col-img4.panel-gas(src='../../images/3panel/panel-round-color4.png')
							img.panel__panels-2-col-img.panel__panels-2-col-img5.panel-gas(src='../../images/3panel/panel-round-color5.png')
							img.panel__panels-2-col-img.panel__panels-2-col-img6.panel-gas(src='../../images/3panel/panel-round-color6.png')
							
